{
  "name": "libinc-hubot",
  "version": "0.0.1",
  "private": true,
  "author": "LiB Inc.",
  "keywords": [
    "github",
    "hubot",
    "libinc",
    "slack",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/libinc/hubot-scripts/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/libinc/hubot-scripts.git"
  },
  "dependencies": {
    "aws-sdk": "^2.0.23",
    "chart.js": "^2.2.2",
    "cheerio": "^0.22.0",
    "githubot": "^1.0.0-beta2",
    "hubot": ">= 2.6.0 < 3.0.0",
    "hubot-scripts": ">= 2.5.0 < 3.0.0",
    "sugar": "^2.0.1"
  },
  "main": "main.coffee",
  "engines": {
    "node": ">= 0.8.x",
    "npm": ">= 1.1.x"
  },
  "scripts": {
    "test": "mocha",
    "coveralls": "mocha test --require blanket --reporter mocha-lcov-reporter | coveralls"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "coveralls": "^2.11.2",
    "hubot-mock-adapter": "^1.0.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "1.2.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "hubot-scripts/scripts",
        "hubot-scripts/module"
      ],
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules"
    }
  }
}
